<%= form_for :dump, :url=>{:action=>"import_csv"}, :html => { :multipart => true } do |f| %>
  <table>
    <tr>
      <td>
        <label for="users_csv_file">
          Select a CSV File:
        </label>
         <%= f.file_field :file %>
      </td>
    </tr>
    <tr>
      <td>
        <%= submit_tag 'Submit' %>
      </td>
    </tr>
  </table>
<% end %>
